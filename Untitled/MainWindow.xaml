<Window x:Name="mainWnd" x:Class="Untitled.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:untitled="clr-namespace:Untitled"
        Title="Files" Height="480" Width="640" MinWidth="600" MinHeight="400" UseLayoutRounding="True" SnapsToDevicePixels="True" TextOptions.TextHintingMode="Fixed" Style="{StaticResource mainwnd}" Icon="StaticResources/Icons/floppy-512.ico" ResizeMode="CanResizeWithGrip" WindowStartupLocation="CenterScreen">
    
    <DockPanel x:Name="mainWndViewRoot" >
        <untitled:Menubar x:Name="menubar" DockPanel.Dock="Top" Margin="4,4,4,4" />
        <untitled:Toolbar x:Name="toolbar" DockPanel.Dock="Top" Margin="4,0,4,6" />
        <untitled:Statusbar x:Name="statusbar" DockPanel.Dock="Bottom" Margin="4,0,4,0" />
        <DockPanel x:Name="pane_1_ViewRoot" DockPanel.Dock="Left">
            <untitled:MillerColumnsLayout x:Name="pane_1_LayoutViewRoot" />
        </DockPanel>
    </DockPanel>
    
</Window>
