<Window x:Name="mainWnd" x:Class="Untitled.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:untitled="clr-namespace:Untitled"
        Title="Files" Height="390" Width="528" MinWidth="400" MinHeight="400" UseLayoutRounding="True" SnapsToDevicePixels="True" TextOptions.TextHintingMode="Fixed" ResizeMode="CanResizeWithGrip" Style="{StaticResource mainwnd}">
    
    <DockPanel x:Name="rootView" >
        <untitled:Menubar x:Name="menubar" DockPanel.Dock="Top" Margin="4,4,4,4"></untitled:Menubar>
        <untitled:Toolbar x:Name="toolbar" DockPanel.Dock="Top" Margin="4,0,4,4"></untitled:Toolbar>
        <untitled:Statusbar x:Name="statusbar" DockPanel.Dock="Bottom" Margin="4,4,4,4"></untitled:Statusbar>
        <DockPanel x:Name="paneViewRoot_1" DockPanel.Dock="Left">
            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Hidden">
                <untitled:MillerColumnsLayout x:Name="paneView_1">
                </untitled:MillerColumnsLayout>
            </ScrollViewer>
        </DockPanel>
    </DockPanel>
    
</Window>
